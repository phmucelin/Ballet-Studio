{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/dist/index.cjs"
        },
        {
            "src": "/(.*)",
            "dest": "/dist/public/$1"
        }
    ],
    "buildCommand": "npm run build",
    "outputDirectory": "dist/public",
    "installCommand": "npm install"
}