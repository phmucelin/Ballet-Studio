{
    "version": 2,
    "builds": [
        {
            "src": "dist/index.cjs",
            "use": "@vercel/node"
        },
        {
            "src": "dist/public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/assets/(.*)",
            "dest": "/dist/public/assets/$1"
        },
        {
            "src": "/api/(.*)",
            "dest": "/dist/index.cjs"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/dist/index.cjs"
        }
    ]
}